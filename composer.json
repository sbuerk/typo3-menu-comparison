{
	"name": "julianhofmann/typo3-menu-comparison",
	"description": "A comparison of different ways to create menus in TYPO3.",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"version": "1.0.0",
	"require": {
		"b13/menus": "dev-task/v13@dev",
		"julianhofmann/sitepackage": "@dev",
		"typo3/cms-backend": "dev-main@dev",
		"typo3/cms-belog": "dev-main@dev",
		"typo3/cms-beuser": "dev-main@dev",
		"typo3/cms-core": "dev-main@dev",
		"typo3/cms-extbase": "dev-main@dev",
		"typo3/cms-extensionmanager": "dev-main@dev",
		"typo3/cms-filelist": "dev-main@dev",
		"typo3/cms-fluid": "dev-main@dev",
		"typo3/cms-fluid-styled-content": "dev-main@dev",
		"typo3/cms-frontend": "dev-main@dev",
		"typo3/cms-info": "dev-main@dev",
		"typo3/cms-install": "dev-main@dev",
		"typo3/cms-lowlevel": "dev-main@dev",
		"typo3/cms-recordlist": "dev-main@dev",
		"typo3/cms-setup": "dev-main@dev",
		"typo3/cms-tstemplate": "dev-main@dev"
	},
	"require-dev": {
		"perftools/php-profiler": "^1.1"
	},
	"repositories": {
		"core": {
			"type": "path",
			"url": "core/typo3/sysext/*"
		},
		"packages": {
			"type": "path",
			"url": "./packages/*"
		}
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"platform": {
			"php": "8.2"
		},
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"ignore-as-root": false,
			"web-dir": ".Build/public"
		}
	},
	"minimum-stability": "dev"
}
